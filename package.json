{
  "name": "@mshima/generator",
  "version": "5.0.0-beta.0",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "homepage": "http://yeoman.io",
  "author": "Yeoman",
  "files": [
    "lib"
  ],
  "main": "lib",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "yeoman",
    "app"
  ],
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.2.7",
    "mem-fs": "^1.1.3",
    "mocha": "^7.2.0",
    "mockery": "^2.1.0",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "prettier": "^1.17.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.2",
    "tui-jsdoc-template": "^1.2.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "license": "BSD-2-Clause",
  "repository": "mshima/generator",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "nyc mocha",
    "doc": "jsdoc -c jsdoc.json",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^4.0.0",
    "cli-table": "^0.3.1",
    "cross-spawn": "^7.0.3",
    "dargs": "^7.0.0",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "diff": "^4.0.2",
    "error": "^7.2.1",
    "find-up": "^4.1.0",
    "github-username": "^5.0.1",
    "istextorbinary": "^2.5.1",
    "lodash": "^4.17.15",
    "make-dir": "^3.1.0",
    "minimist": "^1.2.5",
    "pretty-bytes": "^5.3.0",
    "read-chunk": "^3.2.0",
    "read-pkg-up": "^5.0.0",
    "rimraf": "^3.0.2",
    "run-async": "^2.4.1",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4",
    "text-table": "^0.2.0",
    "through2": "^3.0.1"
  },
  "optionalDependencies": {},
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
